{
    "applications": {
        "gecko": {
            "id": "enhancedimageviewer@sblask",
            "strict_min_version": "53.0"
        }
    },
    "author": "Sebastian Blask",
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "background.js"
        ]
    },
    "description": "Enhanced Image Viewer",
    "homepage_url": "https://github.com/sblask/webextension-enhanced-image-viewer",
    "icons": {
        "128": "icon-128x128.png",
        "16": "icon-16x16.png",
        "32": "icon-32x32.png",
        "48": "icon-48x48.png",
        "64": "icon-64x64.png"
    },
    "manifest_version": 2,
    "name": "Enhanced Image Viewer",
    "permissions": [
        "<all_urls>",
        "webNavigation",
        "webRequest"
    ],
    "version": "1.0.0"
}
